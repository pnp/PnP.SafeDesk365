{
    "type": "AdaptiveCard",
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.3",
    "body": [
        {
            "type": "Container",
            "items": [
                {
                    "type": "TextBlock",
                    "text": "${$root.strings.BookingConfirmed}${bookingId}",
                    "wrap": true
                },
                {
                    "type": "Image",
                    "style": "default",
                    "url": "data:image/png;base64,",
                    "altText": "${$root.strings.BookingDone}",
                    "size": "Small"
                }
            ]
        }
    ],
    "actions": [
      {
          "type": "Action.Submit",
          "title": "${$root.strings.DoneAction}",
          "id": "Submit",
          "data": {
          }
      }
    ]
}